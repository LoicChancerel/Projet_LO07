{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
{% for article in articles %}
<article>
    <h2><a class="articleTitle" href="{{ path('article', { 'id': article.getId() }) }}">{{ article.getTitle() }}</a></h2>
    <p>{{ article.getText() }}</p>
</article>
{% endfor %}
{% endblock %}